\n","s1 = pd.Series([5000, 8000, 10000, 6000, 4000, 5000 ], name='Monthly Salary')\n","\n","print(df1, \"\\n\\n\", s1)"],"metadata":{"id":"od-nqZnL187j","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1753157338275,"user_tz":300,"elapsed":45,"user":{"displayName":"Deepak Singla","userId":"13441456654775539606"}},"outputId":"1df17688-77e0-4e21-926b-e32dfaed4516"},"execution_count":305,"outputs":[{"output_type":"stream","name":"stdout","text":["    Name  Height  Age      Address Qualification\n","0   Adam     5.7   26       Dallas           Msc\n","1    Bob     6.2   32       Boston            MA\n","2  Chris     5.8   29  Little Rock           Msc\n","3    Dan     5.6   27      Atlanta           Msc\n","4  Ethan     5.9   35      Memphis            ME\n","5  Frank     6.0   41    St. Louis           PhD \n","\n"," 0     5000\n","1     8000\n","2    10000\n","3     6000\n","4     4000\n","5     5000\n","Name: Monthly Salary, dtype: int64\n"]}]},{"cell_type":"code","source":["rst=pd.concat([df1,s1], axis=1)\n","rst"],"metadata":{"id":"JCPyxSDA18_7","colab":{"base_uri":"https://localhost:8080/","height":238},"executionInfo":{"status":"ok","timestamp":1753157351721,"user_tz":300,"elapsed":125,"user":{"displayName":"Deepak Singla","userId":"13441456654775539606"}},"outputId":"e2c7c8f9-fc2a-4e12-d6ac-9250bb0e4498"},"execution_count":306,"outputs":[{"output_type":"execute_result","data":{"text/plain":["    Name  Height  Age      Address Qualification  Monthly Salary\n","0   Adam     5.7   26       Dallas           Msc            5000\n","1    Bob     6.2   32       Boston            MA            8000\n","2  Chris     5.8   29  Little Rock           Msc           10000\n","3    Dan     5.6   27      Atlanta           Msc            6000\n","4  Ethan     5.9   35      Memphis            ME            4000\n","5  Frank     6.0   41    St. Louis           PhD            5000"],"text/html":["\n","  <div id=\"df-709287cf-0017-4639-a41f-4